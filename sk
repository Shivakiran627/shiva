class calculator {
    int a;

    public int add(int n1, int n2) {
        int r = n1 + n2;
        return r;
    }
}

public class object1 {
    public static void main(String[] args) {
        int num1 = 10;
        int num2 = 20;

        calculator calc;
        calculator shiva = new calculator();
        shiva.add();
        int result = shiva.add(4, 5);
        // int result=num1+num2;
        System.out.println(result);
    }

}
